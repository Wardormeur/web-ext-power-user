{
	"manifest_version": 2,
	"name": "web-power-user",
	"description": "adding shortcuts to generic UI elements",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"background": {
		"scripts": [
			"background_script.js"
		]
	},
	"permissions": ["contextMenus", "menus", "activeTab"],
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"content_script.js"
			]
		}
	],
	"browser_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "web-power-user"
	},
	"page_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "pageAction/index.html",
		"default_title": "web-power-user"
	},
	"options_ui": {
		"page": "options/index.html"
	}
}